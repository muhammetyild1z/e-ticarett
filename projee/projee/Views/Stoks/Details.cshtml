@model projee.Models.Stok

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="        text-align: center; color: #ee768fbd">Stok detayı</h2>

<div>
   
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <h6 style="margin-left:25px;color: #ee768fbd ">Stok kayıt tarihi</h6>
        </dt>

        <dd style="margin-left:15px">
            @Html.DisplayFor(model => model.CDate)
        </dd>

        <dt>
            <h6 style="margin-left:25px;color: #ee768fbd ">Stok kodu</h6>
        </dt>
        
        <dd style="margin-left:15px">
            @Html.DisplayFor(model => model.Stokkodu)
        </dd>

        <dt>
            <h6 style="margin-left:25px;color: #ee768fbd ">Stok adı</h6>
        </dt>

        <dd style="margin-left:15px">
            @Html.DisplayFor(model => model.Stokadi)
        </dd>

        <dt>
            <h6 style="margin-left:25px;color: #ee768fbd ">Silindi mi</h6>
        </dt>

        <dd style="margin-left:15px">
            @Html.DisplayFor(model => model.Silindimi)
        </dd>

    </dl>
</div>
<p style="margin-left:40px;margin-top:20px">
    @Html.ActionLink("Düzenle", "Edit", new { id = Model.Stokid }) |
    @Html.ActionLink("Geri dön", "Index")
</p>
